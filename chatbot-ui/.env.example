# Production Environment Configuration
# Copy this file to .env.production for production builds

# API Configuration
REACT_APP_API_BASE_URL=https://your-production-api.com

# Feature Flags
REACT_APP_DEBUG=false
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# External Services
REACT_APP_ERROR_REPORTING_URL=https://your-error-service.com/api/errors
REACT_APP_ANALYTICS_ID=your-analytics-id

# Application Settings
REACT_APP_MAX_MESSAGE_LENGTH=1000
REACT_APP_REQUEST_TIMEOUT=30000
REACT_APP_MAX_CONVERSATIONS=10

# Security
REACT_APP_ENABLE_CORS_VALIDATION=true
REACT_APP_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
