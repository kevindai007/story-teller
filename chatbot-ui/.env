# Environment Variables for Chatbot App

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080

# Optional: Enable debug mode
REACT_APP_DEBUG=false

# Optional: Enable analytics (set to true in production)
REACT_APP_ENABLE_ANALYTICS=false

# Optional: Error reporting service URL
REACT_APP_ERROR_REPORTING_URL=

# Optional: Maximum message length
REACT_APP_MAX_MESSAGE_LENGTH=1000

# Optional: Request timeout (in milliseconds)
REACT_APP_REQUEST_TIMEOUT=30000
