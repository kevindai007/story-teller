# Production Environment Variables for Chatbot App
# Copy this file to .env.production for production builds

# API Configuration - Update this to your production API URL
REACT_APP_API_BASE_URL=https://app.kevinonline.org

# Disable debug mode in production
REACT_APP_DEBUG=false

# Enable analytics in production
REACT_APP_ENABLE_ANALYTICS=true

# Error reporting service URL (e.g., Sentry, LogRocket)
REACT_APP_ERROR_REPORTING_URL=

# Production settings
REACT_APP_MAX_MESSAGE_LENGTH=1000
REACT_APP_REQUEST_TIMEOUT=30000

# Optional: API key for authenticated requests
# REACT_APP_API_KEY=your-api-key-here
